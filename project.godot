[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

locale_filter=[ 1, [ "da", "de", "en", "tr" ] ]
translations=PoolStringArray( "res://trans/ceviriler.csv", "res://trans/ceviriler.da.translation", "res://trans/ceviriler.de.translation", "res://trans/ceviriler.en.translation", "res://trans/ceviriler.tr.translation" )
